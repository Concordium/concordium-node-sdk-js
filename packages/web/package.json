{
    "name": "@concordium/web-sdk",
    "version": "6.4.0",
    "license": "Apache-2.0",
    "sideEffects": false,
    "type": "module",
    "browser": "./lib/umd/concordium.min.js",
    "cdn": "./lib/umd/concordium.min.js",
    "types": "./lib/esm/index.d.ts",
    "exports": {
        ".": {
            "types": "./lib/esm/index.d.ts",
            "import": "./lib/esm/index.js",
            "browser": "./lib/esm/index.js"
        },
        "./*": {
            "types": "./lib/esm/pub/*.d.ts",
            "import": "./lib/esm/pub/*.js",
            "browser": "./lib/esm/pub/*.js"
        }
    },
    "files": [
        "lib/esm/**/*",
        "lib/umd/**/*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Concordium/concordium-node-sdk-js",
        "directory": "packages/web"
    },
    "author": {
        "name": "Concordium Software",
        "email": "support@concordium.software",
        "url": "https://concordium.com"
    },
    "browserslist": {
        "production": [
            "chrome >= 67",
            "edge >= 79",
            "firefox >= 68",
            "opera >= 54",
            "safari >= 14",
            "ie >= 100"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "babel-jest": "^27.0.6",
        "eslint": "^7.29.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^29.6.2",
        "rimraf": "^5.0.1",
        "ts-jest": "^29.1.1",
        "typescript": "^5.2.2",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "@concordium/common-sdk": "9.4.0",
        "@grpc/grpc-js": "^1.3.4",
        "@protobuf-ts/grpcweb-transport": "^2.8.2",
        "buffer": "^6.0.3",
        "process": "^0.11.10",
        "stream": "npm:stream-browserify@^3"
    },
    "scripts": {
        "build": "tsc -p tsconfig.build.json && yarn webpack",
        "build-dev": "yarn build",
        "webpack": "node --loader ts-node/esm ../../node_modules/.bin/webpack --config webpack.config.ts",
        "clean": "rimraf -- lib .webpack-cache",
        "lint": "eslint . --cache --ext .ts,.tsx --max-warnings 0",
        "lint-fix": "yarn --silent lint --fix; exit 0",
        "test": "jest"
    }
}
